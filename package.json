{
  "name": "sniktau",
  "version": "0.0.1",
  "description": "Pulls data from Strava and determines all summits in the US you have summited.",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean-and-copy && NODE_ENV=development webpack --progress --env.dev",
    "build:prod": "npm run clean-and-copy && NODE_ENV=production webpack --progress --env.prod",
    "start": "npm run clean-and-copy && NODE_ENV=development babel-node ./src/server/server.js",
    "start:deploy": "PORT=3002 NODE_ENV=production nohup babel-node ./src/server/server.js",
    "test": "npm run prettify && webpack --bail --progress --env.prod && npm run lint && npm run test:unit:coverage",
    "test:unit": "bin/run-tests.sh",
    "test:unit:coverage": "bin/run-tests-coverage.sh",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "clean-dist": "rm -rf ./dist && mkdir dist",
    "copy-index": "cp src/index.html dist",
    "copy-assets": "cp -R src/assets dist",
    "clean-and-copy": "npm run clean-dist && npm run copy-index && npm run copy-assets",
    "lint": "./bin/lint.sh js && ./bin/lint.sh jsx",
    "prettify": "bin/prettify.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jjwyse/sniktau.git"
  },
  "author": "jjwyse",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jjwyse/sniktau/issues"
  },
  "homepage": "https://github.com/jjwyse/sniktau#readme",
