    "react": "^15.3.0",
    "react-ace": "^3.7.0",
    "react-addons-css-transition-group": "^15.3.1",
    "react-dom": "^15.2.1",
    "react-fullstory-component": "git+https://github.com/aaronfullerton/react-fullstory-component.git",
    "react-ga": "^2.1.2",
    "react-imageloader": "^2.1.0",
